{
  "name": "stage-api",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "yarn run barrels && tsc --project tsconfig.compile.json && tsc-alias --project tsconfig.compile.json",
    "barrels": "barrelsby --config .barrelsby.json",
    "start": "NODE_ENV=production yarn run barrels && tsnd -r tsconfig-paths/register --inspect --exit-child --cls --ignore-watch node_modules --respawn --transpile-only src/index.ts",
    "start:prod": "cross-env NODE_ENV=production node dist/index.js",
    "test": "yarn run test:coverage && yarn run test:unit",
    "test:unit": "cross-env NODE_ENV=test jest",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "eslint '**/*.{ts,js}' --fix",
    "prettier": "prettier '**/*.{ts,js,json,md,yml,yaml}' --write",
    "seed": "tsed run seed",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@tsed/ajv": "^7.69.4",
    "@tsed/cli-core": "5.1.2",
    "@tsed/common": "^7.69.4",
    "@tsed/core": "^7.69.4",
    "@tsed/di": "^7.69.4",
    "@tsed/engines": "^7.69.4",
    "@tsed/exceptions": "^7.69.4",
    "@tsed/json-mapper": "^7.69.4",
    "@tsed/logger": "^6.7.5",
    "@tsed/logger-file": "^6.7.5",
    "@tsed/mongoose": "^7.69.4",
    "@tsed/openspec": "^7.69.4",
    "@tsed/platform-cache": "^7.69.4",
    "@tsed/platform-exceptions": "^7.69.4",
    "@tsed/platform-express": "^7.69.4",
    "@tsed/platform-log-middleware": "^7.69.4",
    "@tsed/platform-middlewares": "^7.69.4",
    "@tsed/platform-params": "^7.69.4",
    "@tsed/platform-response-filter": "^7.69.4",
    "@tsed/platform-views": "^7.69.4",
    "@tsed/schema": "^7.69.4",
    "@tsed/swagger": "^7.69.4",
    "ajv": "^8.13.0",
    "barrelsby": "^2.8.1",
    "body-parser": "^1.20.2",
    "cache-manager-ioredis": "^2.1.0",
    "cache-manager-mongoose": "^1.0.1",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "dotenv-expand": "^11.0.6",
    "dotenv-flow": "^4.1.0",
    "express": "^4.19.2",
    "ioredis": "^5.4.1",
    "method-override": "^3.0.0",
    "mongoose": "^8.4.0",
    "patch-package": "^8.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.24.6",
    "@babel/plugin-syntax-decorators": "^7.24.6",
    "@eslint/eslintrc": "eslint/eslintrc",
    "@eslint/js": "^9.3.0",
    "@swc/core": "^1.5.7",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@tsed/cli-plugin-eslint": "5.1.2",
    "@tsed/cli-plugin-mongoose": "5.1.2",
